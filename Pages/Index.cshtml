@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var hospitals = (QuickType.Hospital[]) ViewData["Hospital"];
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <hr/>
    <table border="1" width="90%">
        <tr>
            <th>health_center </th>
            <th>health_center_code</th>
            <th>address</th>
            <th>street_name</th>
            <th>sufx</th>
            <th>city</th>
            <th>state</th>
            <th>zipcode</th>
            <th>phone</th>
            <th>care_type</th>
            <th>monday_open</th>
            <th>monday_close</th>
            <th>tuesday_open</th>
            <th>tuesday_close</th>
            <th>wednesday_open</th>
            <th>wednesday_close</th>
            <th>thursday_open</th>
            <th>thursday_close</th>
            <th>friday_open</th>
            <th>friday_close</th>
            <th>services_provided</th>
            <th>center_description</th>
            <th>insurance_accepted</th>
            <th>title_x_services</th>
            <th>neighborhood</th>
            <th>latitude</th>
            <th>longitude</th>
            <th>adult</th>
            <th>pediatrics</th>
            <th>ob</th>
            <th>nursing_services</th>
            <th>lab</th>
            <th>immunizations</th>
            <th>outreach_enrollment_services</th>
            <th>gyn</th>
            <th>sti_testing</th>
            <th>pharmacy</th>
            <th>diabetic_education</th>
            <th>wic</th>
            <th>behavioral_health</th>
            <th>safe_place</th>
        </tr>
        @{
            foreach (QuickType.Hospital hospital in hospitals)
            {
                <tr>
                    <td>@hospital.HealthCenter</td>
                    <td>@hospital.HealthCenterCode</td>
                    <td>@hospital.Address</td>
                    <td>@hospital.StreetName</td>
                    <td>@hospital.Sufx</td>
                    <td>@hospital.City</td>
                    <td>@hospital.State</td>
                    <td>@hospital.Zipcode</td>
                    <td>@hospital.Phone</td>
                    <td>@hospital.CareType</td>
                    <td>@hospital.MondayOpen</td>
                    <td>@hospital.MondayClose</td>
                    <td>@hospital.TuesdayOpen</td>
                    <td>@hospital.TuesdayClose</td>
                    <td>@hospital.WednesdayOpen</td>
                    <td>@hospital.WednesdayClose</td>
                    <td>@hospital.ThursdayOpen</td>
                    <td>@hospital.ThursdayClose</td>
                    <td>@hospital.FridayOpen</td>
                    <td>@hospital.FridayClose</td>
                    <td>@hospital.ServicesProvided</td>
                    <td>@hospital.CenterDescription</td>
                    <td>@hospital.InsuranceAccepted</td>
                    <td>@hospital.TitleXServices</td>
                    <td>@hospital.Neighborhood</td>
                    <td>@hospital.Longitude</td>
                    <td>@hospital.Latitude</td>
                    <td>@hospital.Adult</td>
                    <td>@hospital.Pediatrics</td>
                    <td>@hospital.Ob</td>
                    <td>@hospital.NursingServices</td>
                    <td>@hospital.Lab</td>
                    <td>@hospital.Immunizations</td>
                    <td>@hospital.OutreachEnrollmentServices</td>
                    <td>@hospital.Gyn</td>
                    <td>@hospital.StiTesting</td>
                    <td>@hospital.Pharmacy</td>
                    <td>@hospital.DiabeticEducation</td>
                    <td>@hospital.Wic</td>
                    <td>@hospital.BehavioralHealth</td>
                    <td>@hospital.SafePlace</td>
                </tr>
            }
        }
    </table>
   
</div>
